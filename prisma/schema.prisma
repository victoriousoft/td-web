generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model Post {
    id        Int      @id @default(autoincrement())
    title     String   @db.TinyText
    content   String   @db.Text
    createdAt DateTime @default(now())
}

model Admin {
    email String @id
}
